{
  "version": 2,
  "builds": [
    {

      "src": "front/package.json", 
      "use": "@vercel/static-build",
      "config": {

        "buildCommand": "npm install && npm run build",
        
        "distDir": "dist" 
      }
    }
  ],
  "routes": [

    {
      "src": "/api/(.*)",
      "dest": "https://tukuy-pacha-2.onrender.com/api/$1"
    },

    {
      "src": "/(.*)",
      "dest": "front/dist/$1"
    }
  ]
}
